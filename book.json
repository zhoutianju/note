{
    "title": "个人笔记",
    "author": "zhoutianju",
    "description": "个人笔记",
    "gitbook" : "3.2.3",
    "plugins": [
        "-sharing",

        "-search",
        "-lunr",
        "search-plus",

        "-highlight",
        "prism",

        "addcssjs",
        "edit-link",
        "anchors",
        "copy-code-button",
        "alerts",
        "anchor-navigation-ex",
        "theme-api"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "edit-link": {
            "base": "https://github.com/zhoutianju/note/edit/master",
            "label": "编辑本页"
        },
        "diff-page": {
            "doc": "这个插件是自己写的，代码在 website.js 中",
            "base": "https://github.com/zhoutianju/note/commits/master/",
            "label": "近期变更"
        },
        "build-page": {
            "doc": "这个插件是自己写的，代码在 website.js 中",
            "base": "http://localhost:23333/",
            "label": "编译"
        },
        "anchor-navigation-ex": {
            "showLevel": true,
            "printLog": true,
            "isShowTocTitleIcon": true,
            "associatedWithSummary": false,
            "multipleH1": false,
            "mode": "float"
        },
        "addcssjs": {
            "js": ["js/website.js", "js/vendors/jquery.scrollTo.min.js"]
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-okaidia.css"
            ]
        }
    },
    "styles": {
        "website": "./styles/website.css"
    },
    "language": "zh-hans"
}
